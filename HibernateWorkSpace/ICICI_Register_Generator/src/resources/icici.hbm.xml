<!DOCTYPE hibernate-mapping PUBLIC  
          "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
          "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">  
<hibernate-mapping>
<class name="beans.ICICIBean" table="iciciaccount" schema="SYSTEM">
<id name="acno">
<generator class="beans.ICICIAccGenerator">
</generator>
